@page "/settings"
@implements IDisposable
@using chd.CaraVan.UI.Components.Pages.Cards

<ServiceState Token="this._cts.Token" Service="pitunnel" />

<Reboot Token="this._cts.Token" />
@code {
    private CancellationTokenSource _cts = new();


    public void Dispose()
    {
        this._cts.Cancel();
        this._cts.Dispose();
    }
}